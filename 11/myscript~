#!/usr/bin/python

import sys

def calc(a, b, c):
    total = str(a * b + c)
    mysum = 0
    for c in total:
        mysum += int(c)


count = int(sys.stdin.readline())

for i in range(count):
    vals = sys.stdin.readline().split()
    vals_f = [int(x) for x in vals]
    print vals_f
    print calc(vals_f[0], vals_f[1], vals_f[2]),
            
